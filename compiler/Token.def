
#ifndef TOKEN
#define TOKEN(X)
#endif
#ifndef PUNCTUATOR
#define PUNCTUATOR(X, Y) TOKEN(X)
#endif
#ifndef KEYWORD
#define KEYWORD(X, Y) TOKEN(X)
#endif

// Punctuator
PUNCTUATOR(LeftSquare,  "[")
PUNCTUATOR(RightSquare, "]")
PUNCTUATOR(LeftParen,   "(")
PUNCTUATOR(RightParen,  ")")
PUNCTUATOR(LeftBrace,   "{")
PUNCTUATOR(RightBrace,  "}")
PUNCTUATOR(Period,      ".")
PUNCTUATOR(Comma,       ",")
PUNCTUATOR(Colon,       ":")
PUNCTUATOR(SemiColon,   ";")
PUNCTUATOR(Arrow,       "->")
PUNCTUATOR(Equal,       "=")

// Keyword
KEYWORD(Def,    "def")
KEYWORD(Func,   "func")
KEYWORD(Int,    "int")
KEYWORD(Return, "return")
KEYWORD(Var,    "var")

#undef KEYWORD
#undef PUNCTUATOR
#undef TOKEN
